<div class="content">
  <h1>Reports</h1>
  <% reports.group_by {|r| r[:academic_year]}.each do |year, y_reports| %>
    <h2><%= year %></h2>
    <% y_reports.each do |report| %>
      <a href="<%= report.identifier.without_ext + '.pdf' %>">Report <%= report[:date].strftime('%d %B %Y') %></a><br>
    <% end %>
  <% end %>
</div>
